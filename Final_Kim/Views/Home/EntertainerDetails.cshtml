@{
}

@model Entertainer

<div class="container">
    <ul class="list-group">
        <li class="list-group-item">@Model.EntStageName</li>
        <li class="list-group-item">@Model.EntSsn</li>
        <li class="list-group-item">@Model.EntStreetAddress</li>
        <li class="list-group-item">@Model.EntCity</li>
        <li class="list-group-item">@Model.EntState</li>
        <li class="list-group-item">@Model.EntZipCode</li>
        <li class="list-group-item">@Model.EntPhoneNumber</li>
        <li class="list-group-item">@Model.EntWebPage</li>
        <li class="list-group-item">@Model.EntEmailAddress</li>
        <li class="list-group-item">@Model.DateEntered</li>
    </ul>
</div>

<br /><br />
<a class="btn btn-dark" asp-controller="Home" asp-action="Edit" asp-route-id="@Model.EntertainerId">Edit</a>
<a class="btn btn-danger" asp-controller="Home" asp-action="Delete" asp-route-id="@Model.EntertainerId">Delete</a>